{
  "kind": "collectionType",
  "collectionName": "nav_links",
  "info": {
    "singularName": "nav-link",
    "pluralName": "nav-links",
    "displayName": "NavLink"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "path": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "iconKey": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "iconImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": 0
    },
    "externalUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "openInNewTab": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "internalPage": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::nav-link.nav-link"
    },
    "menu": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::navigation-menu.navigation-menu"
    }
  }
}
